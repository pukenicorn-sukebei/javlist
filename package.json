{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "scripts": {
    "dotenv": "dotenv -e .env -e .env.dev -e .env.default --",
    "postinstall": "test $SKIP_POSTINSTALL || is-ci || husky install",
    "prisma:generate": "dotenv prisma generate",
    "migration:create": "dotenv prisma migrate dev && dotenv yarn run migration:pull",
    "migration:pull": "dotenv prisma db pull",
    "migration:reset": "dotenv prisma migrate reset",
    "migration:run": "dotenv prisma migrate deploy",
    "generate": "dotenv yarn prisma generate && yarn generate:client",
    "generate:client": "dotenv yarn generate:porn-scraper-client",
    "generate:porn-scraper-client": "openapi-generator-cli generate -g typescript-nestjs --additional-properties=serviceSuffix=ScraperService,supportsES6=true -i https://p.pukenicorn.com/scraper/openapi.json -o generated/porn-scraper-client",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.179.0",
    "@aws-sdk/s3-request-presigner": "^3.186.0",
    "@fastify/swagger": "^8.0.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.1.4",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.1.4",
    "@nestjs/graphql": "^10.1.3",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mercurius": "^10.1.3",
    "@nestjs/platform-fastify": "^9.1.4",
    "@nestjs/swagger": "^6.1.2",
    "@prisma/client": "^4.4.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.1",
    "cache-manager": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.5",
    "fastify": "^4.8.1",
    "graphql": "^16.6.0",
    "mercurius": "^11.0.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.1.4",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/bull": "^3.15.9",
    "@types/cache-manager": "^4.0.2",
    "@types/jest": "^29.1.2",
    "@types/node": "^18.8.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "dotenv-cli": "^6.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^29.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prisma": "^4.4.0",
    "supertest": "^6.3.0",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
