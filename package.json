{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "scripts": {
    "dotenv": "dotenv -e .env -e .env.dev -e .env.default --",
    "postinstall": "test $SKIP_POSTINSTALL || (is-ci && yarn generate || husky install)",
    "prisma:generate": "dotenv prisma generate",
    "migration:create": "dotenv prisma migrate dev && dotenv yarn run migration:pull",
    "migration:pull": "dotenv prisma db pull",
    "migration:reset": "dotenv prisma migrate reset",
    "migration:run": "dotenv prisma migrate deploy",
    "generate": "dotenv yarn prisma generate",
    "generate:spec": "nest start --entryFile generate-openapi",
    "generate:client-ts-axios": "make genc-axios",
    "generate:client-cs-netcore": "make genc-cs-netcore",
    "generate:ts-post": "tsc -p tsconfig.generated.json && rimraf generated-ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.264.0",
    "@aws-sdk/s3-request-presigner": "^3.229.0",
    "@fastify/swagger": "^8.3.0",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/graphql": "^10.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mercurius": "^10.1.7",
    "@nestjs/platform-fastify": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@prisma/client": "^4.9.0",
    "@pukenicorn-sukebei/porn-scraper-client": "^1.0.2",
    "axios": "^1.2.1",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "cache-manager": "^5.1.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "fastify": "^4.11.0",
    "graphql": "^16.6.0",
    "mercurius": "^11.5.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.4",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.1.1",
    "rxjs": "^7.5.7",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0",
    "yaml-js": "^0.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.2.1",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/jest": "^29.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.11.18",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv-cli": "^6.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.4.1",
    "jest-mock-extended": "^3.0.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.7.1",
    "prisma": "^4.9.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tslib": "^2.4.0",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
