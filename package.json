{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "scripts": {
    "dotenv": "dotenv -e .env -e .env.dev -e .env.default --",
    "postinstall": "test $SKIP_POSTINSTALL || (is-ci && yarn generate || husky install)",
    "typeorm-ts": "APP_ENV=cli ts-node -P tsconfig.json ./node_modules/typeorm/cli.js",
    "migration:generate": "dotenv -- yarn run typeorm-ts migration:generate -d src/data-source.ts ",
    "migration:run": "dotenv -- yarn run typeorm-ts migration:run -d src/data-source.ts",
    "generate:spec": "APP_ENV=cli nest start --entryFile generate-openapi",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.332.0",
    "@aws-sdk/s3-request-presigner": "^3.332.0",
    "@fastify/swagger": "^8.3.1",
    "@mercuriusjs/gateway": "^1.2.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/cache-manager": "^1.0.0",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.1",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mercurius": "^11.0.5",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/swagger": "^6.3.0",
    "@nestjs/typeorm": "^9.0.1",
    "@pukenicorn-sukebei/porn-scraper-client": "^1.0.4",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "fastify": "^4.17.0",
    "graphql": "^16.6.0",
    "mysql": "^2.18.1",
    "nexus": "^1.3.0",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.15",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^9.0.0",
    "yaml-js": "^0.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.2.0",
    "@nestjs/testing": "^9.3.12",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.5",
    "dotenv-cli": "^7.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
