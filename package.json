{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "scripts": {
    "dotenv": "dotenv -e .env -e .env.dev -e .env.default --",
    "postinstall": "test $SKIP_POSTINSTALL || (is-ci && yarn generate || husky install)",
    "prisma:generate": "dotenv prisma generate",
    "migration:create": "dotenv prisma migrate dev && dotenv yarn run migration:pull",
    "migration:pull": "dotenv prisma db pull",
    "migration:reset": "dotenv prisma migrate reset",
    "migration:run": "dotenv prisma migrate deploy",
    "generate": "dotenv yarn prisma generate",
    "generate:spec": "nest start --entryFile generate-openapi",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.315.0",
    "@aws-sdk/s3-request-presigner": "^3.321.1",
    "@fastify/swagger": "^8.3.1",
    "@mercuriusjs/gateway": "^1.2.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mercurius": "^11.0.5",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "^4.12.0",
    "@pukenicorn-sukebei/porn-scraper-client": "^1.0.2",
    "axios": "^1.3.5",
    "bcryptjs": "^2.4.3",
    "bull": "^4.10.4",
    "cache-manager": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dayjs": "^1.11.7",
    "fastify": "^4.15.0",
    "graphql": "^16.6.0",
    "nexus": "^1.3.0",
    "nexus-prisma": "^1.0.5",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.0",
    "rxjs": "^7.8.0",
    "source-map-support": "^0.5.21",
    "uuid": "^9.0.0",
    "yaml-js": "^0.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@nestjs/cli": "^9.4.2",
    "@nestjs/schematics": "^9.1.0",
    "@nestjs/testing": "^9.3.12",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.2",
    "@types/cron": "^2.0.1",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.47.0",
    "dotenv-cli": "^7.2.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "prisma": "^4.12.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
