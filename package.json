{
  "name": "nestjs-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "test $SKIP_POSTINSTALL || is-ci || husky install",
    "prisma:generate": "prisma generate",
    "migration:create": "prisma migrate dev && yarn run migration:pull",
    "migration:pull": "prisma db pull",
    "migration:reset": "prisma migrate reset",
    "migration:run": "prisma migrate deploy",
    "generate": "yarn run prisma generate && yarn run generate:client",
    "generate:client": "yarn run generate:porn-scraper-client",
    "generate:porn-scraper-client": "npx @openapitools/openapi-generator-cli generate -g typescript-nestjs --additional-properties=serviceSuffix=ScraperService,supportsES6=true -i https://p.pukenicorn.com/scraper/openapi.json -o src/clients/porn-scraper",
    "prebuild": "rimraf dist",
    "build": "npx @nestjs/cli build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx @nestjs/cli start",
    "start:dev": "npx @nestjs/cli start --watch",
    "start:debug": "npx @nestjs/cli start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.179.0",
    "@aws-sdk/s3-request-presigner": "^3.186.0",
    "@fastify/swagger": "^7.6.0",
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "9.1.4",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.2",
    "@nestjs/graphql": "^10.1.2",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mercurius": "^10.1.2",
    "@nestjs/platform-fastify": "^9.1.4",
    "@nestjs/swagger": "6.1.2",
    "@prisma/client": "^4.3.1",
    "bcryptjs": "2.4.3",
    "cache-manager": "^4.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.5",
    "fastify": "^4.8.1",
    "graphql": "^16.6.0",
    "mercurius": "^11.0.1",
    "nestjs-pino": "^3.1.1",
    "nexus": "^1.3.0",
    "nexus-prisma": "^0.35.0",
    "pg": "8.8.0",
    "pino-http": "^8.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "7.5.7",
    "source-map-support": "0.5.21",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.4",
    "@nestjs/schematics": "9.0.3",
    "@nestjs/testing": "9.1.1",
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/cache-manager": "^4.0.2",
    "@types/jest": "29.0.3",
    "@types/node": "18.8.5",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.40.0",
    "env-cmd": "10.1.0",
    "eslint": "8.24.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.1",
    "is-ci": "3.0.1",
    "jest": "29.0.3",
    "prettier": "2.7.1",
    "prisma": "^4.4.0",
    "supertest": "6.2.4",
    "ts-jest": "29.0.2",
    "ts-loader": "9.4.1",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.1.0",
    "tsconfig-paths-jest": "^0.0.1",
    "tslib": "^2.4.0",
    "typescript": "4.8.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
